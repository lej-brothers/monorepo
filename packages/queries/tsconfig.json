{
  "extends": "tsconfig/base.json",
  "compilerOptions": {
    "outDir": "build",
    "target": "ES6",
    "lib": [
      "DOM",
      "ES6",
      "DOM.Iterable",
      "ScriptHost",
      "ES2016.Array.Include"
    ],
  },
  "include": [
    "./src"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "exclude": [
    "dist",
    "build",
    "node_modules"
  ]
}